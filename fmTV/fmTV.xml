<?xml version="1.0"?>
<Container version="2">
  <Name>fmTV</Name>
  <Repository>mmagtech/fmtv-downloader:latest</Repository>
  <Registry/>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support/>
  <Project>https://github.com/MMagTech/fmTV</Project>
  <Overview/>
  <Category>MediaApp:Video MediaApp:Music</Category>
  <WebUI/>
  <TemplateURL/>
  <Icon>https://raw.githubusercontent.com/MMagTech/fmTV/main/assets/icon.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1718812737</DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Requires/>
  <Config Name="LASTFM_USERNAME" Target="LASTFM_USERNAME" Default="" Mode="" Description="" Type="Variable" Display="always" Required="true" Mask="false"/>
  <Config Name="LASTFM_API_KEY" Target="LASTFM_API_KEY" Default="" Mode="" Description="" Type="Variable" Display="always" Required="true" Mask="true"/>
  <Config Name="POLLING_INTERVAL" Target="POLLING_INTERVAL" Default="" Mode="" Description="Polling Value In Seconds - Looks for last downloaded video and scrobbled after it is downloadedd" Type="Variable" Display="always" Required="true" Mask="false"/>
  <Config Name="/downloads" Target="/downloads" Default="" Mode="rw" Description="Path for Final Output After Processing" Type="Path" Display="always" Required="true" Mask="false"/>
  <Config Name="/appdata" Target="/appdata" Default="" Mode="rw" Description="" Type="Path" Display="always" Required="false" Mask="false"/>
  <Config Name="DOWNLOAD_PATH" Target="DOWNLOAD_PATH" Default="" Mode="" Description="Temporary Folder Variable for Downloads. Do not Alter" Type="Variable" Display="advanced" Required="false" Mask="false">/downloads</Config>
  <Config Name="APP_DATA_PATH" Target="APP_DATA_PATH" Default="" Mode="" Description="AppData Variable for Persistent Data. Do not Alter" Type="Variable" Display="advanced-hide" Required="false" Mask="false">/appdata</Config>
</Container>